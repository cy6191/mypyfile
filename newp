def product(x, *y):   #接收一个或多个数并计算乘积
    s=x
    for n in y:
       s=s*n
return s


class User(Base):   #筛选语句，不是很理解
  __tablename__='user'
  
   id=Column(string(20),primary_key=true)
   name=Column(String(20))
   
books=relationship('Book')

class Book(Base):
     __tablename__='book'
     
     id=Column(String(20),primary_key=true)
     name=Column(String(20))
     
     user_id=Column(String(20),ForeignKey('user.id'))
